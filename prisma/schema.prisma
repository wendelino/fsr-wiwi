generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model DB_Location {
  label String @id
  lat   Float
  long  Float
}

model Event_DB {
  title       String   @id
  description String
  start       DateTime
  end         DateTime
}
